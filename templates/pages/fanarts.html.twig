{% extends "base.html.twig" %}

{% block body %}

    <div class="contenuElements">
        {% for fanArt in fanArts %}
            <div class="elements">
                <p>{{ fanArt.fanArtTitle }}</p>
                <img src="{{ fanArt.fanArtSketch }}"></img>
                <div class="text">
                    <p>{{ fanArt.fanArtHook }}</p>
                </div>
            </div>
        {% endfor %}
    </div>
{# Modifier contenuElements #}
{% endblock %}